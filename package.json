{
  "name": "vertical-tabs",
  "version": "1.0.1",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A vertical tabs made in the material design style. It uses paper-tab as a tab UI.",
  "devDependencies": {
    "polymer-cli": "^0.17.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "vertical-tabs.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint vertical-tabs.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"vertical-tabs:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/vertical-tabs.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/vertical-tabs/issues",
    "email": "arc@mulesoft.com"
  }
}
